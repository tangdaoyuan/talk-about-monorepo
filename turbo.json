{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "build/**"
      ]
    },
    "dev": {
      "dependsOn": [
        "^build"
      ],
      "cache": false,
      "outputs": []
    },
    "dev:zh": {
      "dependsOn": [
        "^build"
      ],
      "cache": false,
      "outputs": []
    },
    "play": {
      "dependsOn": [
        "^build"
      ],
      "cache": false,
      "outputs": []
    }
  }
}
